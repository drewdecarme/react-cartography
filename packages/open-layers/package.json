{
  "name": "@react-cartography/ol",
  "version": "0.0.0",
  "description": "A collection of lower level hooks to easily integrate Open Layers into your NextJS, Remix, Vite React app",
  "type": "module",
  "scripts": {
    "dev": "yarn build --watch",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "ol": "10.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "22.4.2",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.10",
    "typescript": "5.5.4",
    "typescript-eslint": "8.2.0"
  }
}
